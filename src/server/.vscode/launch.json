// A launch configuration that compiles the extension and then opens it inside a new window
{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Attach to Server",
			"type": "node",
			"request": "attach",
			"port": 6005,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/../../out/server"
		}
    ]
}